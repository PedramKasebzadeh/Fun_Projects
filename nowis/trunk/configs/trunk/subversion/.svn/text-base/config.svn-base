[auth]
# password-stores = gnome-keyring,kwallet
# kwallet-wallet =
# kwallet-svn-application-name-with-pid = yes
store-passwords = no
# store-auth-creds = no

[helpers]
editor-cmd = vim 
diff-cmd = vimdiff
diff3-cmd = vimdiff 
diff3-has-program-arg = no # accepts '--diff-program' option
# merge-tool-cmd = merge_command

[tunnels]

[miscellany]
global-ignores = *.o *.lo *.la *.al .libs *.so *.so.[0-9]* *.a *.pyc *.pyo *.rej *~ #*# .#* .*.swp .DS_Store
log-encoding = utf-8
enable-auto-props = yes
interactive-conflicts = yes

[auto-props]
*.c = svn:eol-style=native
*.cpp = svn:eol-style=native
*.h = svn:eol-style=native
*.sh = svn:eol-style=native;svn:executable
*.txt = svn:eol-style=native
*.png = svn:mime-type=image/png
*.jpg = svn:mime-type=image/jpeg
Makefile = svn:eol-style=native
*.html = svn:mime-type=text/html;svn:eol-style=native
*.htm = svn:mime-type=text/html;svn:eol-style=native
*.gif = svn:mime-type=image/gif
*.rtf = svn:mime-type=text/rtf
*.ai = svn:mime-type=application/illustrator
